        {
          "id": "start",
          "label": "üé® Let's start",
          "value": "start",
          "next": "b3"
        },
        {
          "id": "more-info",
          "label": "ü§î Tell me more first",
          "value": "more-info",
          "next": "b2-info"
        }
      ]
    },
    "b2-info": {
      "id": "b2-info",
      "type": "message-button",
      "content": "Great question! GHAC has been championing the arts in our region for over 50 years. We support individual artists, connect communities through creative experiences, and advocate for the arts as essential to our quality of life.\n\nYour input helps us understand what matters most to the people who care about arts and culture in Greater Hartford. There are no wrong answers‚Äîjust your honest perspective!\n\nThis conversation is completely confidential, and you can skip any question you'd prefer not to answer.",
      "buttonText": "Let's go!",
      "next": "b3"
    },
    "b3": {
      "id": "b3",
      "type": "text-input",
      "content": "What should I call you? (Or just hit enter to skip)",
      "placeholder": "First name",
      "variable": "user_name",
      "required": false,
      "next": "b4",
      "onEmpty": {
        "message": "No problem! Let's get started.",
        "next": "b4"
      }
    },
    "b4": {
      "id": "b4",
      "type": "single-choice",
      "content": "{{#if user_name}}Thanks, {{user_name}}!{{else}}Thanks!{{/if}} What's your connection to Greater Hartford Arts Council?",
      "variable": "connection_type",
      "options": [
        {
          "id": "current-supporter",
          "label": "I currently support GHAC",
          "value": "current-supporter",
          "next": "b4a",
          "setVariables": {
            "is_current_supporter": true,
            "is_past_supporter": false,
            "show_barriers": false,
            "show_giving_level": true
          }
        },
        {
          "id": "past-supporter",
          "label": "I've supported GHAC in the past",
          "value": "past-supporter",
          "next": "b4a",
          "setVariables": {
            "is_current_supporter": false,
            "is_past_supporter": true,
            "show_barriers": true,
            "show_giving_level": true
          }
        },
        {
          "id": "workplace",
          "label": "I participated through a workplace giving campaign",
          "value": "workplace",
          "next": "b4a",
          "setVariables": {
            "is_current_supporter": false,
            "is_past_supporter": false,
            "show_barriers": true,
            "show_giving_level": false
          }
        },
        {
          "id": "connected-artist",
          "label": "I'm connected through an artist or arts organization",
          "value": "connected-artist",
          "next": "b4a",
          "setVariables": {
            "is_current_supporter": false,
            "is_past_supporter": false,
            "show_barriers": true,
            "show_giving_level": false
          }
        },
        {
